<div class="w-full">
  <% if notice.present? %>
   <%= render "shared/notifications"%>
  <% end %>

  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl">Participants</h1>
    <%= link_to "New participant", new_participant_path, class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium" %>
  </div>

  <div id="participants" class="min-w-full">
    <%= render @participants %>
  </div>
</div>
